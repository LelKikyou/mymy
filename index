ASAS
</body>
<script>
window.onclick=function (e) {
        stopDefault( e )
    };
    function stopDefault( e ) {
        //阻止默认浏览器动作(W3C)
        if ( e && e.preventDefault )
            e.preventDefault();
        //IE中阻止函数器默认动作的方式
        else
            window.event.returnValue = false;
        return false;
    }
</script>
</html>
